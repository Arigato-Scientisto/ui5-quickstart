{
    "_version": "1.1.0",
    "sap.app": {
            "_version": "1.1.0",
            "id": "<component ID>",
            "type": "application",
            "i18n": "resources/i18n/texts.properties",
            "title": "{{<key in .properties file>}}",
            "description": "{{<key in .properties file>}}",
            "applicationVersion": {
                    "version": "${project.version}"
            },
            "ach": "<ach>",
            "dataSources": {
                    "PathPersistenceServiceRoot": {
                            "uri": "<URI of path persistence>",
                            "type": "OData",
                            "settings": {
                                    "odataVersion": "2.0"
                            }
                    },
                    "SmartBusiness": {
                            "uri": "/sap/hba/r/sb/core/odata/runtime/SMART_BUSINESS.xsodata",
                            "type": "OData",
                            "settings": {
                                    "odataVersion": "2.0"
                            }
                    },
                    "LogicalSystem": {
                            "uri": "/sap/hba/apps/wca/dso/s/odata/wca.xsodata",
                            "type": "OData",
                            "settings": {
                                    "odataVersion": "2.0"
                            }
                    }
            },
            "resources": "resources.json",
            "offline": false
    },
    "sap.fiori": {
            "_version": "1.1.0",
            "registrationIds": [
                    "<FioriId>"
            ],
            "archeType": "analytical"
    },
    "sap.ui": {
            "_version": "1.1.0",
            "technology": "UI5",
            "deviceTypes": {
                    "desktop": true,
                    "tablet": true,
                    "phone": false
            },
            "supportedThemes": [
                    "sap_hcb",
                    "sap_bluecrystal"
            ]
    },
    "sap.ui5": {
            "_version": "1.1.0",
            "dependencies": {
                    "minUI5Version": "1.38.1",
                    "libs": {
                            "sap.ui.core": {
                                    "minVersion": "1.38.1"
                            },
                            "sap.ca.ui": {
                                    "minVersion": "1.38.1"
                            },
                            "sap.m": {
                                    "minVersion": "1.38.1"
                            },
                            "sap.ui.layout": {
                                    "minVersion": "1.38.1"
                           },
                            "sap.ushell": {
                                    "minVersion": "1.38.1"
                            },
                            "sap.apf": {
                                    "minVersion": "1.38.1"
                            },
                            "sap.viz": {
                                    "minVersion": "1.38.1"
                            }
                    }
            },
            "extends": {
                    "component": "sap.apf.base"
            },
            "contentDensities": {
                    "compact": true,
                    "cozy": true
            },
            "models": {
                    "i18n": {
                            "type": "sap.ui.model.resource.ResourceModel",
                            "uri": "resources/i18n/texts.properties"
                    }
            }
    }
}